{
	"name": "visually",
	"version": "1.0.0",
	"description": "Song lyric analysis and data visualization tool",
	"repository": "https://github.com/evanwinter/visually",
	"main": "index.html",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "parcel index.html",
		"build": "rm -rf dist/ && parcel build index.html",
		"format": "prettier --write 'ui/**/*.js' 'core/**/*.js'",
		"clean": "rm -rf dist/ && rm -rf .cache/ && rm -rf .parcel-cache/"
	},
	"author": "Evan Winter",
	"license": "MIT",
	"dependencies": {
		"@nivo/line": "^0.61.1",
		"idb-keyval": "^3.2.0",
		"promise-worker": "^2.0.1",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-feather": "^2.0.3",
		"react-redux": "^7.1.3",
		"react-transition-group": "^4.3.0",
		"redux": "^4.0.5",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0",
		"svg-loaders-react": "^2.2.1"
	},
	"devDependencies": {
		"parcel": "^2.0.0-nightly.114",
		"sass": "^1.25.0"
	},
	"alias": {
		"core": "./src/core",
		"ui": "./src/ui",
		"components": "./src/ui/components",
		"hooks": "./src/ui/hooks",
		"styles": "./src/ui/styles",
		"workers": "./src/core/workers"
	}
}
